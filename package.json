    {
      "path": "./dist/css/bootstrap.css",
      "maxSize": "25 kB"
    },
    {
      "path": "./dist/css/bootstrap.min.css",
      "maxSize": "23 kB"
    },
    {
      "path": "./dist/js/bootstrap.bundle.js",
      "maxSize": "47 kB"
    },
    {
      "path": "./dist/js/bootstrap.bundle.min.js",
      "maxSize": "22 kB"
    },
    {
      "path": "./dist/js/bootstrap.js",
      "maxSize": "25 kB"
    },
    {
      "path": "./dist/js/bootstrap.min.js",
      "maxSize": "15.5 kB"
    }
  ]
